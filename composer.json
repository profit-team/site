{
    "name": "smart-core/smart-core-cms-sandbox",
    "description": "Smart Core CMS Sandbox",
    "license": "MIT",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.4.11",
        "ext-intl": "*",
        "ext-curl": "*",
        "symfony/symfony": "2.6.*",
        "symfony/assetic-bundle": "dev-master",
        "symfony/swiftmailer-bundle": "*",
        "symfony/monolog-bundle": "*",
        "sensio/distribution-bundle": "*",
        "sensio/generator-bundle": "*",

        "twig/extensions": "~1.0",

        "doctrine/orm": "~2.5",
        "doctrine/doctrine-cache-bundle": "dev-master",
        "doctrine/migrations": "1.0.*@dev",
        "doctrine/doctrine-migrations-bundle": "2.1.*@dev",
        "doctrine/doctrine-bundle": "~1.5",
        "doctrine/doctrine-fixtures-bundle": "dev-master",

        "asm89/twig-cache-extension": "*",
        "amazonwebservices/aws-sdk-for-php": "*",
        "aws/aws-sdk-php": "*",
        "dizda/cloud-backup-bundle": "dev-master",
        "dropbox-php/dropbox-php": "*",
        "egeloen/ordered-form": "~1.0",
        "egulias/email-validator": "*",
        "egulias/listeners-debug-command-bundle": "*",
        "elao/web-profiler-extra-bundle": "dev-master",
        "fabpot/goutte": "dev-master",
        "facebook/php-sdk": "*",
        "friendsofsymfony/rest-bundle": "*",
        "friendsofsymfony/user-bundle": "dev-master",
        "genemu/form-bundle": "*",
        "helios-ag/fm-elfinder-bundle": "~4.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "intervention/image": "*",
        "jms/debugging-bundle": "dev-master",
        "jms/di-extra-bundle": "*",
        "jms/serializer-bundle": "*",
        "jms/security-extra-bundle": "*",
        "hakre/dropbox-uploader": "*",
        "happyr/google-site-authenticator-bundle": "*",
        "happyr/slugify-bundle": "*",
        "hwi/oauth-bundle": "0.4.*@dev",
        "knplabs/knp-disqus-bundle":"dev-master",
        "knplabs/knp-menu": "dev-master",
        "knplabs/knp-menu-bundle": "dev-master",
        "knplabs/knp-paginator-bundle": "*",
        "knplabs/rad-bundle": "*",
        "leafo/lessphp": "*",
        "leafo/scssphp": "*",
        "liip/imagine-bundle": "*",
        "liip/functional-test-bundle": "dev-master",
        "liip/theme-bundle": "dev-master",
        "misd/guzzle-bundle": "~1.0",
        "mopa/bootstrap-bundle":  "dev-master",
        "mremi/templating-extra-bundle": "dev-master",
        "pagerfanta/pagerfanta": "v1.0.1",
        "predis/predis": "0.8.*",
        "rickysu/tagcache-bundle": "dev-master",
        "snc/redis-bundle": "1.1.*",
        "smart-core/accelerator-cache-bundle": "dev-master",
        "smart-core/core-bundle": "dev-master",
        "smart-core/felib-bundle": "dev-master",
        "smart-core/ladybug-bundle": "~0.7",
        "smart-core/session-bundle": "dev-master",
        "sonata-project/intl-bundle": "dev-master",
        "sp/bower-bundle": "*",
        "stfalcon/tinymce-bundle": "dev-master",
        "whiteoctober/breadcrumbs-bundle": "dev-master",
        "white-october/pagerfanta-bundle": "dev-master",
        "zendframework/zend-servicemanager": "*",
        "zendframework/zend-tag": "*",
        "zenstruck/imagine-extra-bundle": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "sh bin/init_var",
            "sh bin/clear_cache",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "SmartCore\\Bundle\\AcceleratorCacheBundle\\Composer\\ScriptHandler::clearCache",
            "php app/console doctrine:schema:update"
        ],
        "post-update-cmd": [
            "sh bin/init_var",
            "sh bin/clear_cache",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "SmartCore\\Bundle\\AcceleratorCacheBundle\\Composer\\ScriptHandler::clearCache",
            "php app/console doctrine:schema:update"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
    "minimum-stability": "alpha",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "symlink",
        "branch-alias": {
            "dev-master": "2.6-dev"
        },
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
